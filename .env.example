# Example Environment Configuration
# Copy this file to .env.local and customize for your environment

# Application
VITE_APP_NAME="Structural Analysis System"
VITE_APP_VERSION="1.0.0"
VITE_APP_DESCRIPTION="Professional Structural Analysis and Design Tool"

# Environment
VITE_ENV=development

# API Configuration (if needed)
VITE_API_BASE_URL=http://localhost:3000/api
VITE_API_TIMEOUT=30000

# Features
VITE_ENABLE_DEBUG=true
VITE_ENABLE_PERFORMANCE_MONITORING=false
VITE_ENABLE_ERROR_REPORTING=false
VITE_ENABLE_ANALYTICS=false

# Security
VITE_ENABLE_AUDIT_LOGGING=true
VITE_MAX_FILE_SIZE=10485760
VITE_ALLOWED_FILE_TYPES=".json,.csv,.txt"

# Performance
VITE_ENABLE_CACHING=true
VITE_MAX_STRUCTURE_SIZE=1000

# UI
VITE_DEFAULT_THEME=light
VITE_ENABLE_DARK_MODE=true