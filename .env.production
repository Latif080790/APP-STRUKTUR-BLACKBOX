# Environment Configuration
# Production Environment

# Application
VITE_APP_NAME="Structural Analysis System"
VITE_APP_VERSION="1.0.0"
VITE_APP_DESCRIPTION="Professional Structural Analysis and Design Tool"

# Environment
NODE_ENV=production
VITE_ENV=production

# API Configuration
VITE_API_BASE_URL=https://api.structuralanalysis.com/api
VITE_API_TIMEOUT=30000

# Features
VITE_ENABLE_DEBUG=false
VITE_ENABLE_PERFORMANCE_MONITORING=true
VITE_ENABLE_ERROR_REPORTING=true
VITE_ENABLE_ANALYTICS=true

# Security
VITE_ENABLE_CSP=true
VITE_ENABLE_AUDIT_LOGGING=true
VITE_MAX_FILE_SIZE=10485760
VITE_ALLOWED_FILE_TYPES=".json,.csv,.txt"

# Performance
VITE_ENABLE_CACHING=true
VITE_CACHE_DURATION=86400000
VITE_MAX_STRUCTURE_SIZE=5000

# UI
VITE_DEFAULT_THEME=light
VITE_ENABLE_DARK_MODE=true
VITE_ENABLE_ANIMATIONS=true